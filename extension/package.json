{
  "name": "vscode-custom-loader",
  "displayName": "VS Code Custom Loader",
  "description": "Allow loading custom JS and CSS into Main and Renderer Process",
  "author": "nidefawl",
  "publisher": "nidefawl",
  "license": "MIT",
  "version": "0.0.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nidefawl/vscode-custom-loader"
  },
  "homepage": "https://github.com/nidefawl/vscode-custom-loader",
  "main": "vscode-cl-extension.js",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other",
    "Themes"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-custom-loader.patchinstallation",
        "title": "Install required VSCode Patch",
        "category": "VSCode Custom Loader"
      },
      {
        "command": "vscode-custom-loader.reset",
        "title": "Remove all contributions",
        "category": "VSCode Custom Loader"
      },
      {
        "command": "vscode-custom-loader.show",
        "title": "Show all contributions",
        "category": "VSCode Custom Loader"
      }
    ]
  }
}
