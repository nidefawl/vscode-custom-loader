{
  "name": "vscode-custom-loader",
  "workspaces": [
    "extension",
    "hook",
    "vscode-background-image"
  ],
  "description": "VSCode Custom Loader Project",
  "author": "nidefawl",
  "license": "MIT",
  "version": "0.0.3",
  "private": true,
  "repository-todo": {
    "type": "git",
    "url": "https://github.com/nidefawl/xxxx"
  },
  
  "scripts": {
    "lint": "eslint .",
    "dist-custom-loader": "cd extension && vsce package --pre-release --no-git-tag-version --yarn --no-dependencies -o ../dist",
    "dist-background-image": "cd vscode-background-image && vsce package --pre-release --no-git-tag-version --yarn --no-dependencies -o ../dist",
    "dist": "yarn dist-custom-loader && yarn dist-background-image"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.63.1",
    "eslint": "^8.7.0",
    "vsce": "^2.6.4"
  }
}
